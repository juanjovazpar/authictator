{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Fastify",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "serve:debug"],
      "restart": true,
      "protocol": "inspector",
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true
    }
  ]
}
